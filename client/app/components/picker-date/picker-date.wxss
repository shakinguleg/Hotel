.picker{
  width: 100%;
  height: 75px;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #fff;
}
.left,.right{
  width: 100px;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
}
.tit{
  font-size: 12px;
  color: #333;
}
.date{
  font-size: 14px;
  color: #4CB8BE;
}
.center{
  width: 125px;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  box-sizing: border-box;
  padding-top: 5px;
  padding-bottom: 20px;
}
.center image{
  width: 125px;
  height: 20px;
}
.center .duration{
  font-size: 12px;
  color: #4CB8BE;
}