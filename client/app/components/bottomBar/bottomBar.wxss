/* components/bottomBar/bottomBar.wxss */
.bottom_bar {
  width: 100%;
  height: 49px;
  background-color: white;
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  position: fixed;
  bottom: 0;
  left: 0;
}

.bottom_bar_item_wrap {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size: 10px;
}

.bottom_bar_img {
  width: 30px;
  height: 100%;
}